        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f1"
        },
        {
            "actor": "char:toyle",
            "action": "say",
            "text": "About Jasmaby not knowing the mummy was actually a zombie."
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f2"
        },
        {
            "actor": "char:vivian",
            "action": "say",
            "text": "I don't know anything about that. I just owe her one, so I'm gonna melt some faces for her."
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f3"
        },
