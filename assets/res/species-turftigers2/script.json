        {
            "actor": "backdrop",
            "action": "show",
            "view": "2e3"
        },
        {
            "actor": "char:vivian",
            "action": "say",
            "text": "What about you, little guy?"
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2e4"
        },
        {
            "actor": "char:toyle",
            "action": "say",
            "text": "I'm not much of a rocker. Can I have the mic back?"
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2e3"
        },
        {
            "actor": "char:vivian",
            "action": "say",
            "text": "You got it, guy."
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f1"
        },
        {
            "actor": "char:toyle",
            "action": "say",
            "text": "Tell us, why did you pick Charthur's side?"
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f2"
        },
        {
            "actor": "char:vivian",
            "action": "say",
            "text": "I was in the neighborhood and heard she was getting into a badass fight, and I owe her one."
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f1"
        },
        {
            "actor": "char:toyle",
            "action": "say",
            "text": "Do you think she's right?"
        },
        {
            "actor": "backdrop",
            "action": "show",
            "view": "2f2"
        },
        {
            "actor": "char:vivian",
            "action": "say",
            "text": "About what?"
